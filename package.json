  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.dev.js --hot --open --host 0.0.0.0",
    "docker": "docker build -t franz-manager .",
    "build": "NODE_ENV=production webpack --mode production --display-modules --config webpack.prod.js && npm run replace",
    "replace": "npm run replace-js-location && npm run generate-css-themes",
    "replace-js-location": "cp index.html dist/index.html && sed -i 's~/app.js~app.js~' dist/index.html",
    "generate-css-themes": "node plugins/generateCssThemes.js"
  },
  "keywords": [],
  "license": "EPL-2.0",
