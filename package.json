    "@babel/preset-react": "^7.0.0",
    "@mdi/font": "^3.3.92",
    "acorn": "^6.0.5",
    "babel-loader": "^8.0.5",
    "classnames": "^2.2.5",
    "css-loader": "^1.0.0",
    "file-loader": "^3.0.1",
