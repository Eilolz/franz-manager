  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.dev.js --hot --open --host 0.0.0.0",
    "docker": "docker build -t franz-manager .",
    "build": "NODE_ENV=production webpack --mode production --config webpack.prod.js && npm run replace",
    "replace": "npm run replace-js-location && npm run generate-css-themes",
    "replace-js-location": "cp index.html dist/index.html && sed -i 's~<script src=\"/app.js\"></script>~~' dist/index.html",
    "generate-css-themes": "node generateCssThemes.js"
  },
